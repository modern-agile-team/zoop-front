  "info": {
    "title": "quizzes-game-io-backend",
    "description": "The Quizzes Game IO Backend API",
    "version": "0.1",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "/backend"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "SignUpWithUsernameDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": ["username", "password"]
      },
      "AuthTokenDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          }
        },
        "required": ["accessToken"]
      },
      "SignInWithUsernameDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": ["username", "password"]
      },
      "CreateGameRoomDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "게임 방 제목",
            "example": "My Awesome Game Room",
            "maxLength": 50,
            "minLength": 1
          }
        },
        "required": ["title"]
      },
      "GameRoomDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "hostId": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": ["waiting", "ready", "inProgress", "finished", "paused"]
          },
          "title": {
            "type": "string"
          },
          "maxMembersCount": {
            "type": "number"
          },
          "currentMembersCount": {
            "type": "number"
          }
        },
        "required": [
          "id",
          "createdAt",
          "updatedAt",
          "hostId",
          "status",
          "title",
          "maxMembersCount",
          "currentMembersCount"
        ]
      },
      "GameRoomMemberDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "accountId": {
            "type": "string"
          },
          "gameRoomId": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": ["host", "player"]
          }
        },
        "required": [
          "id",
          "createdAt",
          "updatedAt",
          "accountId",
          "gameRoomId",
          "role"
        ]
      },
      "GameRoomCollectionDto": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GameRoomDto"
            }
          }
        },
        "required": ["data"]
      }
    }
  }
}
